        "jestConfig": "libs/identify-shared-characteristics/frontend/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/identify-shared-characteristics/frontend/**/*.ts",
          "libs/identify-shared-characteristics/frontend/**/*.html"
        ]
      }
    }
  }
}
